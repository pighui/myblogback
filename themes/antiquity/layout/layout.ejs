<%- partial('_partial/head') %>
<body>
<div id="fullpage" class="mobile-nav-<%= theme.mobile_nav %>">
    <% if (theme.background_image){ %>
    <div id="wrapper" style="background-image: url(<%= theme.background_image %>)"
         title="<%= theme.background_title ? theme.background_title : '背景图片来自网络' %>">
        <% } else { %>
        <div id="wrapper" title="<%= theme.background_title ? theme.background_title : '背景图片来自网络' %>">
            <% } %>

            <%- partial('_partial/header', null, {cache: !config.relative_link}) %>
            <div id="middlecontent" title class="global-width sidebar-<%= theme.sidebar %>">
                <section id="main"><%- body %></section>
                <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
            </div>
            <%- partial('_partial/footer', null, {cache: !config.relative_link}) %>
            <%- partial('_partial/after-footer') %>
        </div>
        <%- partial('_partial/mobile-nav') %>
    </div>
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script src="/js/love.js"></script>
    <script src="/js/snowy.js"></script>
    <style type="text/css">.snow-container {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            pointer-events: none;
            z-index: 100001;
        }</style>
    <div class="snow-container"></div>
</body>
</html>